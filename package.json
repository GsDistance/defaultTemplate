{
    "name": "gsd-versioner",
    "version": "1.0.0",
    "description": "Default template for quick setup.",
    "main": "dist/index/index.js",
    "scripts": {
      "run": "node dist/index/index.js",
      "build": "ncc build src/index.js -o dist/index --source-map && ncc build src/cleanup.js -o dist/cleanup --source-map && ncc build src/pre.js -o dist/pre --source-map",
      "prepare": "husky install"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/GsDistance/defaultTemplate.git"
    },
    "keywords": [
      "github-actions",
      "versioning",
      "GsDistance"
    ],
    "author": "GsDistance",
    "license": "MIT",
    "dependencies": {
      "@actions/core": "^1.10.0",
      "@actions/github": "^5.1.1"
    },
    "devDependencies": {
      "@vercel/ncc": "^0.36.1",
      "husky": "^8.0.3"
    }
  }